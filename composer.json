{
    "name": "cleaniquecoders/laravel-db2doc",
    "description": "Laravel Db2Doc generate your database schema documentation easily.",
    "keywords": ["package", "laravel", "database", "documentation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\LaravelDB2DOC\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\LaravelDB2DOC\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.7",
        "illuminate/support": "^5.5|^5.6|^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "mockery/mockery": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\LaravelDB2DOC\\LaravelDB2DOCServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
